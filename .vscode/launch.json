    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "[webapp] dev server",
            "request": "launch",
            "projectName": "management.server.app.dev",
            "cwd": "${workspaceFolder}/management.server.app.dev",
            "mainClass": "org.eclipse.scout.dev.jetty.JettyServer"
        },
        {
            "type": "java",
            "name": "[webapp] dev ui",
            "request": "launch",
            "projectName": "management.ui.html.app.dev",
            "cwd": "${workspaceFolder}/management.ui.html.app.dev",
            "mainClass": "org.eclipse.scout.dev.jetty.JettyServer",
            "vmArgs": "-Dscout.jetty.port=8082 -Djandex.rebuild=true"
        }
    ]
}